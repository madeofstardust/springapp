<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "Main_series" table = "Main_series">

        <meta attribute = "class-description">
            This class contains data about Main Series.
        </meta>

        <id name = "ID" type = "int" column = "ID">
            <generator class="native"/>
        </id>

        <many-to-one name="Title_of_a_series" cascade="all"></many-to-one>
       <!-- <property name = "Title_of_a_series" column = "Title_of_a_series" type = "string"/> -->
        <property name = "Magical_or_not" column = "Magical_or_not" type = "string"/>
        <property name = "For_children_or_not" column = "For_children_or_not" type = "string"/>

    </class>
</hibernate-mapping>